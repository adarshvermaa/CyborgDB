# Example environment variables for Docker Compose
# Copy this to .env in the docker directory

# CyborgDB Configuration
CYBORGDB_API_URL=https://api.cyborgdb.com
CYBORGDB_API_KEY=your_cyborgdb_api_key_here

# Encryption (generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))")
ENCRYPTION_KEY=0123456789abcdef0123456789abcdef0123456789abcdef0123456789abcdef

# JWT
JWT_SECRET=your_jwt_secret_key_change_in_production

# Embedding Provider (openai or ollama)
EMBEDDING_PROVIDER=openai

# OpenAI (if using OpenAI)
OPENAI_API_KEY=sk-your-openai-api-key-here

# Ollama (if using Ollama)
OLLAMA_BASE_URL=http://host.docker.internal:11434
